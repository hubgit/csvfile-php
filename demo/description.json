{
  "url": "header.csv",
  "fields": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "volume": {
      "type": "integer"
    },
    "weight (kg)": {
      "type": "integer",
      "property": "weight",
      "unit": "kg"
    },
    "date published": {
      "type": "datetime",
      "format": "YYYY-mm-dd",
      "title": "publication date",
      "description": "the date the item was published"
    }
  },
  "dialect": {
    "delimiter": ",",
    "terminator": "\n",
    "enclosure": "\"",
    "double-quote": false,
    "skip-initial-space": false
  },
  "headers": {
    "rows": 1,
    "columns": 1,
    "skip-rows": 4,
    "skip-columns": 0
  },
  "@context": "http://example.com/context.json"
}
